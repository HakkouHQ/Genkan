{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "semi": 0,
        "max-len": 0,
        "indent": ["error", 4],
        "new-cap": 0,
        "object-curly-spacing": ["error", "always"],
        "prefer-arrow-callback": "error"
    }
}
